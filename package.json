  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/vue-soft-ui-dashboard.git"
  },
  "main": "main.js",
  "build": {
    "directories": {
      "output": "build",
      "app": "."
    },
    "files": [
      "dist/**/*",
      "preload.js",
      "main.js",
      "package.json"
    ]
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cross-env CI=false cross-env PUBLIC_URL=/ vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "electron .",
    "electron:serve": "NODE_ENV=development concurrently \"vue-cli-service serve --port 8080\" \"wait-on http://localhost:8080 && electron .\"",
    "electron:build": "vue-cli-service build && electron-builder"
  },
